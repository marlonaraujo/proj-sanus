<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>
        Prontuário Digital
    </display-name>
    <description>
        Prontuário Digital
    </description>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <filter>
        <filter-name>MedicoFilter</filter-name>
        <filter-class>medico.MedicoFilter</filter-class>
    </filter>
    <filter>
        <filter-name>PacienteFilter</filter-name>
        <filter-class>paciente.PacienteFilter</filter-class>
    </filter>
    <filter>
        <filter-name>AdmFilter</filter-name>
        <filter-class>pessoa.AdmFilter</filter-class>
    </filter>
    <filter>
        <filter-name>PessoaValida</filter-name>
        <filter-class>pessoa.PessoaValida</filter-class>
    </filter>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
